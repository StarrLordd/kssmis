@{
    ViewBag.Title = "ViewProfile";
    Layout = "~/Views/Shared/MZCommonLayout.cshtml";
}
@using MyProject.Models
@using System.Data
@model DataTable


@{
    foreach(DataRow dr in Model.Rows)
    {
        <h1 style="text-align:center;">@dr[2]</h1>
    <div class="col-sm-12">
        <div style="height:200px;width:200px;overflow:hidden;border-radius:50%;">
            <img src="/Content/UserPics/@dr[5]" height="250" class="img-responsive"/>
           
        </div>
        <span style="margin-left:500px;margin-top:-150px"> Enrollment No. : <b>@dr[0]</b></span>
    </div>
    }
    }